    "server": "cross-env NODE_ENV=production nodemon --exec babel-node ./server/index.js",
    "start:client": "cross-env DANGEROUSLY_DISABLE_HOST_CHECK=true node scripts/start.js",
    "start": "npm run build:client && npm run server",
    "build:client": "cross-env GENERATE_SOURCEMAP=false  PUBLIC_URL=/ node scripts/build",
